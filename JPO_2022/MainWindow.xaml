<Window x:Class="JPO_2022.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JPO_2022"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas x:Name="Canvas">
        <Image x:Name="Player1" Source="player.png" Height="70" Width="70" Canvas.Left="{Binding X}" Canvas.Top="{Binding Y}" Visibility="{Binding Visible}"/>
        <Image x:Name="Key" Source="key.png" Height="70" Width="70" Canvas.Left="{Binding X}" Canvas.Top="{Binding Y}" Visibility="{Binding Visible}"/>
    </Canvas>
</Window>
